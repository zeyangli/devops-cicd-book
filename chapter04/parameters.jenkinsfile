pipeline {
    agent any
	parameters { 
        string(name: 'VERSION', defaultValue: '1.1.1', description: '')
        choice choices: ['dev', 'test', 'uat'], description: 'env names', name: 'ENVNAME' 
    }
    stages {
        stage("Build"){
            steps {
                echo "${params.VERSION}"
                echo "${params.ENVNAME}"
            }
        }
    }
}
